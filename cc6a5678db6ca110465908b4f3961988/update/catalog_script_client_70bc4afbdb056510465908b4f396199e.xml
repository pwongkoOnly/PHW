        <name>Set Position Type</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
	if (isLoading || newValue == '') {
		return;
	}
	
	var requestedChg = g_form.getValue('requested_change');
	var reqForLoc    = g_form.getValue('requested_for_location');
	
	if ((reqForLoc == 'Support Office') && (requestedChg != 'Deactivate Profile') ) {
		g_form.setValue('position_type', 'Company');
	} else if ((reqForLoc == 'Store') && (requestedChg != 'Deactivate Profile') ) {
		g_form.setValue('position_type', 'Location');
	}

	g_form.setReadOnly('position_type', true);
   
}]]></script>
